		"@sveltejs/vite-plugin-svelte": "^6.2.0",
		"@types/eslint": "^9.6.1",
		"autoprefixer": "^10.4.21",
		"eslint": "^9.36.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.12.3",
		"postcss": "^8.5.6",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"svelte": "^5.45.10",
		"svelte-check": "^4.3.1",
		"tailwindcss": "^3.4.17",
		"typescript": "^5.9.3",
		"vite": "^7.1.7"
	},
	"type": "module",
	"dependencies": {
